{
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**", "build/**", ".next/**"]
    },
    "format": {
      "cache": false
    },
    "format:check": {
      "cache": true,
      "inputs": ["**/*.{js,jsx,ts,tsx,json,md}"]
    },
    "lint": {
      "dependsOn": ["format:check"],
      "inputs": ["**/*.{js,jsx,ts,tsx}"]
    },
    "lint:fix": {}
  }
}
